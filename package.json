{
  "name": "vue-section-scroller",
  "version": "1.0.10",
  "private": false,
  "license": "MIT",
  "author": "Wojciech Sikora <kontakt@sikorawojciech.pl> (https://sikorawojciech.pl)",
  "description": "Vue.js component for switching sections with scrolling and swiping (for mobile)",
  "homepage": "https://github.com/SikoraWojciech/vue-section-scroller",
  "bugs": {
    "url": "https://github.com/SikoraWojciech/vue-section-scroller/issues",
    "email": "kontakt@sikorawojciech.pl"
  },
  "main": "dist/vue-section-scroller.ssr.js",
  "browser": "dist/vue-section-scroller.esm.js",
  "module": "dist/vue-section-scroller.esm.js",
  "unpkg": "dist/vue-section-scroller.min.js",
  "types": "vue-section-scroller.d.ts",
  "files": [
    "dist/*",
    "vue-section-scroller.d.ts",
    "src/types/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.1.2",
    "cross-env": "^7.0.3",
    "gsap": "^3.7.0",
    "minimist": "^1.2.5",
    "node-sass": "^6.0.0",
    "postcss": "^8.2.3",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.35.1",
    "sass-loader": "^10.2.0",
    "typescript": "^3.8.3",
    "vue": "^3.1.2"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  }
}